#. # Strings2 strings
#: 1.Strings2.HorizontalOrganigram.Label
msgid "Horizontal Organization Chart"
msgstr "Vízszintes szervezeti diagram"

#: 2.Strings2.HorizontalOrganigramDescription.Label
msgid "Use this to illustrate horizontal hierarchical relationships"
msgstr "Vízszintes hierarchikus kapcsolatok szemléltetésére használható"

#: 3.Strings2.TableHierarchyDiagram.Label
msgid "Table Hierarchy"
msgstr "Táblázatos hierarchia diagram"

#: 4.Strings2.TableHierarchyDiagramDescription.Label
msgid ""
"Use this to illustrate groups of information built from top to bottom, and "
"the hierarchies within each group"
msgstr ""
"Információcsoportok ábrázolása fentről lefelé, az egyes csoportokra jellemző "
"hierarchiák szemléltetésével"

#: 5.Strings2.Organigram2.Label
msgid "Organization Chart 2"
msgstr "Szervezeti diagram 2"

#: 6.Strings2.Organigram2Description.Label
msgid ""
"Use this to illustrate hierarchical relationships, below third level items "
"are displayed vertical layout"
msgstr ""
"Vízszintes hierarchikus kapcsolatok szemléltetésére használható, a harmadik "
"szint alatti elemek elrendezése függőleges"

#: 7.Strings2.ButtDiagram.Label
msgid "Target Diagram"
msgstr "Céltábla diagram"

#: 8.Strings2.ButtDiagramDescription.Label
msgid "Use this to illustrate steps toward a goal"
msgstr "Egy végső célkitűzés felé tett lépéseket ábrázolja"

#: 9.Strings2.InactiveFunction.Label
msgid ""
"Inactive function - this option can be used if only simple item has been "
"selected"
msgstr ""
"Nem aktív funkció - a szövegszerkesztő csak akkor aktív, ha csak egyetlen "
"elem van kiválasztva"

#: 10.Strings2.Diagrams.UserDefineDescription.Label
msgid "Personal settings"
msgstr "Egyéni beállítások"

#: 11.Strings2.Organigram.DefaultDescription.Label
msgid ""
"Area: allShape, doesnot modify colors, displays medium rounded rectangles "
"with outline and without shadow"
msgstr ""
